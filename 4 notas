#include <stdio.h>
#include <stdlib.h>
#include <locale.h>

int main (){
	setlocale(LC_ALL,"");
	
	// Declarando Variáveis.
	int nota, soma, i;
	
//solicintando dados para o usuário.
	for (i = 1; i <+ 5; i++){
		printf("Digite a %iºnota: ", i);
		scanf("%i",&nota);
		soma = soma + nota;
	}
	//Exibindo Resultados.
	printf("\nSoma: %i \n",soma);
	
	return 0;
}
