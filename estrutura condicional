programa
{
	
	funcao inicio()
{
		inteiro idade

		escreva("digite sua idade: ")
		leia(idade)

		se (idade > 18) {
			escreva(" maior de idade")
		} senao {
			escreva("menor de idade")
		}

		 escreva("fim do programa")
   
	     }
	    
}

